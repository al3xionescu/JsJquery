<!DOCTYPE html>
<html>
<head>
<script src="https://ajax.googleapis.com/ajax/libs/jquery/3.4.1/jquery.min.js"></script>
</head>
<body>

<h1>Setting Text Content</h1>

<h2 id="01">Hi!</h2>
<h2 id="02">Hello!</h2>

<script>
$(document).ready(function() {
  var myElement = $("#01");
  myElement.text("Hello!");
});
</script>

</body>
</html>


<!DOCTYPE html>
<html>
<head>
<script src="https://ajax.googleapis.com/ajax/libs/jquery/3.4.1/jquery.min.js"></script>
</head>
<body>

<h1>Getting Text Content</h1>

<h2 id="01">Hello Romania!</h2>
<h2 id="02">Hello Sweden!</h2>
<h2 id="03">Hello Japan!</h2>

<p id="trial"></p>

<script>
$(document).ready(function() {
  var myElement = $("#02");
  var myText = myElement.text();
  $("#trial").text(myText); 
});
</script>

</body>
</html>

